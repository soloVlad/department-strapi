{
  "kind": "collectionType",
  "collectionName": "professors",
  "info": {
    "singularName": "professor",
    "pluralName": "professors",
    "displayName": "Professor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true,
      "maxLength": 240,
      "minLength": 2
    },
    "shownInPreview": {
      "type": "boolean",
      "default": false
    },
    "isRetired": {
      "type": "boolean",
      "default": false
    },
    "email": {
      "type": "string",
      "regex": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "degree": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::professor-degree.professor-degree"
    },
    "priority": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 1000
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "number": {
      "type": "biginteger"
    },
    "studentWorks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student-work.student-work",
      "mappedBy": "professor"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "professor"
    }
  }
}
